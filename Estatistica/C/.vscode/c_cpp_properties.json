{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/",
                "/usr/lib/gcc/x86_64-linux-gnu/13/include/",  // Versão 13 do GCC
                "/usr/include/x86_64-linux-gnu/",             // Headers específicos da arquitetura
                "/usr/local/include/"                        // Bibliotecas instaladas manualmente
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",                 // Caminho confirmado
            "intelliSenseMode": "linux-gcc-x64",
            "cppStandard": "c17",
            "browse": {
                "path": [
                    "/usr/include",
                    "/usr/lib/gcc/x86_64-linux-gnu/13/include/", // Atualizado para 13
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}